{
  "name": "venni",
  "version": "1.0.0",
  "description": "Web based chat app",
  "scripts": {
    "start": "serve -s build",
    "dev": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --color --coverage",
    "eject": "react-app-rewired eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovieokeh/venni-app.git"
  },
  "keywords": [
    "firebase",
    "react.js",
    "typescript"
  ],
  "author": "Ovie Okeh",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ovieokeh/venni-app/issues"
  },
  "homepage": "https://github.com/ovieokeh/venni-app#readme",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "antd": "^3.23.2",
    "aos": "^2.3.4",
    "firebase": "^6.6.2",
    "history": "^4.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "serve": "^11.2.0",
    "socket.io-client": "^2.2.0",
    "typescript": "3.6"
  },
  "devDependencies": {
    "@types/aos": "^3.0.2",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/node": "12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "@types/redux-mock-store": "^1.0.1",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "babel-plugin-import": "^1.12.1",
    "customize-cra": "^0.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.3",
    "react-test-renderer": "^16.9.0",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/index.ts",
      "!src/**/index.tsx",
      "!src/routes/*.**",
      "!src/serviceWorker.ts",
      "!src/redux/reducers/rootReducer.ts",
      "!src/redux/types.ts",
      "!src/redux/store.ts",
      "!src/utilities/*.ts"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1"
    }
  }
}
